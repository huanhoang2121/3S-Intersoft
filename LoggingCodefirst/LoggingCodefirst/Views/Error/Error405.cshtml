@using LoggingCodefirst.Resources

@inject LocalizationService Localizer
@{
    ViewBag.Title = Localizer.GetLocalizedString("405");
    Layout = "_ErrorLayout";
}

<div class="error-box">
    <div class="error-body text-center">
        <h1 class="error-title text-danger">@ViewBag.Title</h1>
        <h3 class="text-uppercase">@Localizer.GetLocalizedString("Method not allowed!")</h3>
        <p class="text-muted m-t-30 m-b-30">@Localizer.GetLocalizedString("The requested request method is not accepted on this server.")</p>
        <a href="javascript:history.go(-1)" class="btn btn-danger btn-rounded waves-effect waves-light m-b-40">@Localizer.GetLocalizedString("Back")</a> </div>
</div>